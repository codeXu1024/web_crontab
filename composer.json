{
    "name": "codexu1024/web_crontab",
    "description": "php web 定时任务工具",
    "type": "library",
    "require": {
        "php": ">7.2",
        "workerman/workerman": ">=3.5",
        "topthink/think-orm": "2.0.x-dev",
        "ext-json": "*",
        "workerman/crontab": "dev-main",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "8.0.x-dev",
        "vlucas/phpdotenv": "^5.6@dev"
    },
    "license": "Apache-2.0",
    "suggest": {
      "ext-event": "For better performance. "
    },
    "autoload": {
        "psr-4": {
            "Codexu1024\\WebCrontab\\": "src/"
        },
        "files":[
            "src/Lib/functions.php"
        ]
    },
    "bin":[
        "src/ext/bin/web_crontab_init.php"
    ],
    "minimum-stability": "dev"
}
